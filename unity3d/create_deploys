
echo "creating unitypackage for Unity Soomla Vungle"
/Applications/Unity/Unity.app/Contents/MacOS/Unity \
  -batchmode -logFile create_unity.log \
  -projectPath $PWD/Soomla/ \
  -exportPackage Assets/Plugins/Android Assets/Plugins/iOS Assets/Plugins/Soomla/Plugins Assets/Soomla/ $PWD/soomla-unity3d-vungle.unitypackage \
  -quit
